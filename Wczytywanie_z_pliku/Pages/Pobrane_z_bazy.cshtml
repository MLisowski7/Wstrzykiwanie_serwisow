@page
@model Pobrane_z_bazyModel
@{
    ViewData["Title"] = "Pobrane z bazy";
}
<div class="text-center">
    <h1 class="card-body">Dane pobrane z bazy:</h1>
</div>

<div class="card-columns">
    @foreach (var item in Model.Product)
    {
        <div class-="card">
            <div class="card-img" style="background-image: url('@Html.DisplayFor(modelItem => item.Image)');"></div>
            <div class="card-body">
                <h5 class="card-title"> @Html.DisplayFor(modelItem => item.Title)</h5>
            </div>
        </div>
    }
</div>
